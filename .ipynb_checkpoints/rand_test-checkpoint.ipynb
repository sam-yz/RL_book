    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "import math\n",
    "\n",
    "L = []\n",
    "size = 100\n",
    "const = 0.8\n",
    "for i in range(0, size):\n",
    "    L.append(math.pow(const, size-i))\n",
    "\n",
    "plt.plot(L)\n",
    "plt.show()\n",
